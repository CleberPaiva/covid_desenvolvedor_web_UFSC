<a href="/cadastrar"><button type="button" class="btn btn-success">Novo</button></a>
<p>
<p>Bem vindo!</p>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Nome</th>
            <th scope="col">CPF</th>
            <th scope="col">Sexo</th>
			<th scope="col">Quantidade Materias</th>
        </tr>
    </thead>
    <tbody>
        {{#each aluno}}
        <tr>
            <th>{{nome}}</th>
            <td>{{cpf}}</td>
            <td>{{sexo}}</td>
			<td>{{qtd_materias}}</td>
            <td>
                <a href="/editar/{{cpf}}"><button type="submit" class="btn btn-warning">Alterar</button></a>
                <a href="/remover/{{cpf}}"><button type="submit" class="btn btn-danger">Excluir</button></a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
